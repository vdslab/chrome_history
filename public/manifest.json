{
  "manifest_version": 3,
  "name": "History Tree",
  "version": "0.7.0",

  "description": "履歴の遷移をグラフで描画します",
  "action": { "default_title": "History Tree", "default_popup": "popup.html" },
  "permissions": ["history", "storage"],
  "icons": {
    "16": "images/logo_16.png",
    "32": "images/logo_32.png",
    "48": "images/logo_48.png",
    "128": "images/logo_128.png"
  },

  "background": {
    "service_worker": "js/background.js"
  }
}
