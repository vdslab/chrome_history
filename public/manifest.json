{
  "manifest_version": 3,
  "name": "History Tree",
  "description": "履歴を階層構造っぽく描画する",
  "version": "1.0",
  "action": { "default_title": "History Tree", "default_popup": "popup.html" },
  "permissions": ["history", "tabs", "nativeMessaging", "storage"],
  "icons": {
    "16": "logo_16.png",
    "32": "logo_32.png",
    "48": "logo_48.png",
    "128": "logo_128.png"
  },
  "background": {
    "service_worker": "js/background.js"
  }
}
